<div class="list-group">
{% for em_name, namespaces in tree %}
    {% for namespace, metadatas in namespaces %}
    <div class="list-group-item justify-content-between">
        <em>{{ namespace }}</em>
        <span class="badge badge-default badge-pill">{{ em_name }}</span>
    </div>
        {% for metadata in metadatas %}
        <div class="list-group-item list-group-item-action d-flex justify-content-start">
            <a href="{{ path('dual_structure', {'class': metadata.getParamName()}) }}" class="p-2">
                <i class="fa fa-chevron-circle-right" aria-hidden="true"></i> &nbsp;
                {{ metadata.getShortName() }}
            </a>

            <a class="ml-auto p-2" href="{{ path('dual_data', {'class': metadata.getParamName()}) }}">
                <i class="fa fa-table" aria-hidden="true"></i>
            </a>
        </div>
        {% endfor %}
    {% endfor %}
{% endfor %}
</div>