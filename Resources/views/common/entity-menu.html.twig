<form action="{{ dual_query_url(meta.name) }}" method="get" class="form">
    <div class="btn-toolbar justify-content-between" role="toolbar">
        <div class="btn-group mr-2" role="group">

            <a href="{{ path('dual_data', {'class': meta.paramName}) }}" class="btn btn-primary">
                <i class="fa fa-table" aria-hidden="true"></i>
                Data
            </a>

            <a href="{{ path('dual_structure', {'class': meta.paramName}) }}" class="btn btn-primary">
                <i class="fa fa-sitemap" aria-hidden="true"></i>
                Structure
            </a>

            <a href="{{ path('dual_create', {'class': meta.paramName}) }}" class="btn btn-primary">
                <i class="fa fa-plus-square" aria-hidden="true"></i>
                Create entry
            </a>

        </div>
        <div class="input-group">
            <input type="text" class="form-control" placeholder="Search..." name="q">
            <button type="submit" class="input-group-addon btn btn-primary"><i class="fa fa-search"></i></button>
        </div>
    </div>
</form>